{
  "name": "@struere/runtime",
  "version": "0.1.0",
  "description": "Runtime execution engine for Struere",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./serverless/vercel": {
      "import": "./dist/serverless/vercel.js",
      "types": "./dist/serverless/vercel.d.ts"
    },
    "./serverless/lambda": {
      "import": "./dist/serverless/lambda.js",
      "types": "./dist/serverless/lambda.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@struere/core": "^0.1.0",
    "ai": "^4.1.0",
    "@ai-sdk/anthropic": "^1.1.0",
    "@ai-sdk/openai": "^1.1.0",
    "@ai-sdk/google": "^1.1.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist"
  ],
  "license": "MIT"
}
