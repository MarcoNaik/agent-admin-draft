    "test": "bun test"
  },
  "dependencies": {
    "@agent-factory/core": "workspace:*",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "commander": "^12.0.0",
    "ora": "^8.0.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "bun-types": "^1.0.0",
    "typescript": "^5.3.0"
  }
}
