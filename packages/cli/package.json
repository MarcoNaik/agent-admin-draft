{
  "name": "@agent-factory/cli",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "af": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "files": ["dist"],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node --external commander --external chalk --external ora --external chokidar --external yaml && chmod +x ./dist/index.js",
