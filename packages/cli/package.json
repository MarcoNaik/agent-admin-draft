{
  "name": "@struere/cli",
  "version": "0.2.0",
  "description": "CLI tool for developing, testing, and deploying AI agents",
  "keywords": ["ai", "agents", "cli", "developer-tools", "llm"],
  "author": "struere",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/struere/struere.git",
    "directory": "packages/cli"
  },
  "homepage": "https://struere.dev",
  "bugs": {
    "url": "https://github.com/struere/struere/issues"
  },
  "type": "module",
  "bin": {
    "struere": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "files": ["dist"],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun --external commander --external chalk --external ora --external chokidar --external yaml --external @struere/runtime && chmod +x ./dist/index.js",
    "dev": "tsc --watch",
    "test": "bun test"
  },
  "dependencies": {
    "@struere/core": "^0.1.0",
    "@struere/runtime": "^0.1.0",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "commander": "^12.0.0",
    "ora": "^8.0.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "bun-types": "^1.0.0",
    "typescript": "^5.3.0"
  }
}
